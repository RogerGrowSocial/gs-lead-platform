{
  "version": 2,
  "installCommand": "npm ci",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "functions": {
    "server.js": {
      "maxDuration": 30,
      "memory": 1024,
      "includeFiles": [
        "node_modules/**"
      ]
    }
  }
}
