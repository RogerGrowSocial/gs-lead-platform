{
  "version": 2,
  "installCommand": "npm ci",
  "buildCommand": "echo 'No build step needed'",
  "functions": {
    "api/index.js": {
      "maxDuration": 30,
      "memory": 1024,
      "includeFiles": "**"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.js"
    }
  ]
}
